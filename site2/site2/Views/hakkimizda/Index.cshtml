@using site2.Models.Sınıflar
@model List<hakkimizda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_testLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda bu olacak</h3>

            @foreach (var x in Model)
            {
                <img src="@x.resimUrl" alt="" />
                <p>@x.aciklama </p>

            }
        </div>
    </div>
</div>

